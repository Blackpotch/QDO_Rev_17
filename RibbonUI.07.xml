<?xml version="1.0" encoding="utf-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
  <ribbon startFromScratch="false">
    <tabs>
      <tab id="tabPIRSA" label="PIRSA - QDO">
        <group id="grpQDO" label="QDO">
          <button id="btnNew" label="New" imageMso="RecordsAddFromOutlook" size="large" onAction="NewQDO" />
          <button id="btnEdit" label="Edit" imageMso="TableStyleClear" size="large" onAction="EditQDO" />
          <button id="btnSaveAs" label="Save As" imageMso="Copy" size="large" onAction="SaveQDOAs" />
        </group>
        <group id="grpNav" label="Navigation">
          <button id="btnFirst" label="First" imageMso="ShapeRoundedRectangle" size="large" onAction="MoveFirst" />
          <button id="btnBack" label="Back" imageMso="ShapeLeftArrow" size="large" onAction="MoveBack" />
          <button id="btnForward" label="Forward" size="large" onAction="MoveForward" imageMso="ShapeRightArrow" />
          <button id="btnLast" label="Last" size="large" onAction="MoveLast" imageMso="ShapeRoundedRectangle" />
        </group>
        <group id="grpShare" label="Share">
          <button id="btnPrint" label="Print" size="large" onAction="PrintQDO" imageMso="FilePrint" />
          <button id="btnPrintPreview" label="Print Preview" size="large" onAction="PrintPrevQDO" imageMso="FilePrintPreview" />
          <button id="btnEmail" label="Email" size="large" onAction="EmailQDO" imageMso="FileSendAsAttachment" />
        </group>
        <group id="grpOptions" label="Options">
          <checkBox id="chkHideOldSheetControls" label="Hide Old Sheet Controls" onAction="ToggleSheetControls" getPressed="GetSheetControlStatus" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>